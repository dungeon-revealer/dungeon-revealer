{
  "compilerOptions": {
    "target": "ES2019",
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "module": "CommonJS",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "outDir": "./../server-build",
    "typeRoots": ["../node_modules/@types"]
  },
  "include": ["./"]
}
